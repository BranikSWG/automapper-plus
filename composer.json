{
    "name": "mark-gerarts/auto-mapper-plus",
    "description": "An AutoMapper for PHP",
    "license": "MIT",
    "type": "library",
    "keywords": ["automapper", "converter", "object-to-object"],
    "authors": [
        {
            "name": "Mark Gerarts",
            "email": "mark.gerarts@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AutoMapperPlus\\":  "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AutoMapperPlus\\Test\\": "test/"
        }
    },
    "require": {
        "php": ">=7.2.0",
        "thecodingmachine/phpstan-strict-rules": "^0.12.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.3",
        "symfony/debug": "^3.3",
        "phpstan/phpstan": "^0.12.7",
        "phpstan/phpstan-phpunit": "^0.12.6",
        "phpunit/phpunit": "^8.5",
        "phpstan/phpstan-deprecation-rules": "^0.12.2"
    }
}
