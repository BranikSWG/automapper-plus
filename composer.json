            "AutoMapperPlus\\": "test/"
        }
    },
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.3",
        "symfony/debug": "^3.3",
        "phpunit/phpunit": "^6.4",
        "mark-gerarts/phpstan-automapper-plus": "^0.1.0"
    }
}
